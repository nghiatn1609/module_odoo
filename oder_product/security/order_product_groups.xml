<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record model="ir.module.category" id="module_category_oderproduct">
        <field name="name">Order Product Category</field>
        <field name="description">Helps you to manage the order.</field>
        <field name="sequence">10</field>
    </record>
    <record id="group_order_product_user" model="res.groups">
        <field name="name">Order Product User</field>
        <field name="category_id" ref="oder_product.module_category_oderproduct"/>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
    </record>
    <record id="group_order_product_manager" model="res.groups">
        <field name="name">Order Product Manager</field>
        <field name="category_id" ref="oder_product.module_category_oderproduct"/>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
    </record>
</odoo>