<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="report_order_product_line">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <t t-call="web.external_layout">
                    <div class="page">
                        <h1 t-field="doc.product_id"/>
                        <h1 t-field="doc.uom_id"/>
                        <h1 t-field="doc.total"/>
                    </div>
                </t>
            </t>
        </t>
    </template>
</odoo>