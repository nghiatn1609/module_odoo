<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_product_account_sinvoice_form" model="ir.ui.view">
            <field name="name">product.template.account.sinvoice.form</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//page//group//field[@name='categ_id']" position="after">
                    <field name="release_sinvoice_name"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>